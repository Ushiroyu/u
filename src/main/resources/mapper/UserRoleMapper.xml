<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.community.mapper.UserRoleMapper">
    <select id="getUserRolesIdById" resultType="int">
        select ur.`role_id`
        from `user_role` ur
        where ur.`user_id` = #{userId}
    </select>
</mapper>
